{
  "name": "fd-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:prod": "ng build --configuration production && purgecss --config ./purgecss.config.ts",
    "test": "ng test",
    "lint": "ng lint",
    "gzip": "rm -rf ./dist/*-gzip && gzipper compress --verbose --output-file-format [filename].[ext] ./dist/fd-frontend ./dist/fd-frontend-gzip"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular/animations": "^16.1.8",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.1.8",
    "@angular/google-maps": "^16.1.7",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/router": "^16.1.8",
    "@ng-bootstrap/ng-bootstrap": "^15.0.1",
    "@ngneat/hot-toast": "^5.0.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "hamburgers": "^1.2.1",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
